{
           "Statement": [
               {
                   "Action": "ecr:GetAuthorizationToken",
                   "Effect": "Allow",
                   "Resource": "*"
               },
               {
                   "Action": [
                       "ecr:UploadLayerPart",
                       "ecr:PutImage",
                       "ecr:InitiateLayerUpload",
                       "ecr:GetDownloadUrlForLayer",
                       "ecr:CompleteLayerUpload",
                       "ecr:BatchGetImage",
                       "ecr:BatchCheckLayerAvailability"
                   ],
                   "Effect": "Allow",
                   "Resource": [
                       "arn:aws:ecr:${region}:${account_id}:repository/${deploy_id}/model",
                       "arn:aws:ecr:${region}:${account_id}:repository/${deploy_id}/environment"
                   ]
               },
               {
                   "Action": [
                       "ecr:GetDownloadUrlForLayer",
                       "ecr:CreateRepository",
                       "ecr:BatchImportUpstreamImage",
                       "ecr:BatchGetImage"
                   ],
                   "Effect": "Allow",
                   "Resource": "arn:aws:ecr:${region}:${account_id}:repository/${deploy_id}/quay/*"
               }
           ],
           "Version": "2012-10-17"
       }
       
